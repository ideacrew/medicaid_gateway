<h1>Event Log</h1>
<%= render partial: "date_range", locals: {start_on: @start_on, end_on: @end_on, session_name: ""} %>
<p>Transfers Out: <%= @transfers_total %></p>
<p>Transfers In: <%= @inbound_transfers_total %></p>
<p>Determinations: <%= @determinations_total %></p>
<p>MEC Checks: <%= @mec_checks_total %></p>

<table class="table table-striped table-responsive table-overrides">
	<thead>
		<th>Status</th>
		<th>Type</th>
		<th>Created At</th>
		<th>Application Identifier</th>
	</thead>
	<tbody>
		<% @events.each do |event| %>
			<tr>
				<td class="<%= event[:success] ? 'table-success' : 'table-danger' %>"><%= event[:success] ? "Success" : "Failure" %></td>
				<td><%= event[:type] %></td>
				<td><%= event[:created_at] %></td>
				<td><%= event[:app_id] %></td>
			</tr>
		<% end %>
	</tbody>
</table>