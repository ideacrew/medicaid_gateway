{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSaveTimeout": 5000,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.erb": "erb"
  },
  "[ruby]": {
    "editor.defaultFormatter": "castwide.solargraph"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cSpell.words": [
    "amqp",
    "Anchore",
    "buildx",
    "codeql",
    "rabbitmq",
    "rabbitmqctl",
    "sarif",
    "slackapi",
    "vhost",
    "YELLR",
    "rabbitmq"
  ]
}
